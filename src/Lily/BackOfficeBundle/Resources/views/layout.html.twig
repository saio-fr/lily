    <script src="{{ asset('js/ie/fix.js') }}" cache="false"></script>
  <![endif]-->

  <link rel="icon" href="{{ asset('images/home/favicon-saio.png') }}" />

</head>
<body>

  <section class="vbox">
    
    {% include "LilyBackOfficeBundle::header.html.twig" %}

    {% block body %}{% endblock %}

  </section>

