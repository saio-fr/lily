<div class="checkbox">
  <input type="checkbox" name="select">
</div>
<% if (category) { %>
  <span class="question-category-label" style="background: #<%= category.color %>">
    <i class="icon-folder-open-alt"></i>
    <%= category.title %>
  </span>
<% } %>
<h5 class="title"><%= title %></h5>
<span class="informations">
  Posée <%= requests %> fois - Modifié le 
  <%= moment(date).format('DD/MM/YY') %> par <%= modifiedBy %>
</span>