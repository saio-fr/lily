		  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

	    {% if app.environment == 'dev' %}
		  ga('create', 'UA-58973224-1', 'auto');
	    {% else %}
			ga('create', 'UA-58975501-1', 'auto');
	    {% endif %}
