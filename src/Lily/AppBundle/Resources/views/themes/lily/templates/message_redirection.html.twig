<ul>
<% if (config.hasTel) { %>
	<i class="lily-redirection-item lily-redirection-mail">
		<a href="#/mail"><%= copy.mail %>
			<span class="lily-icon lily-icon-arrow-right"></span>
		</a>
	</i>
<% } %>

<% if (config.hasMail)  { %>
	<i class="lily-redirection-item lily-redirection-tel">
		<a href="tel:<%= redirections.tel %>">	<%= copy.tel %>
			<span class="lily-icon lily-icon-arrow-right"></span>
		</a>
	</i>
<% } %>

<% if (config.hasChat && config.chatAvailable)  { %>
	<i class="lily-redirection-item lily-redirection-chat">
		<a href="#/chat">
			<%= copy.chat %>
			<span class="lily-icon lily-icon-arrow-right"></span>
		</a>
	</i>
<% } %>
</ul>
