<div class="lily-page-cont"> <!--wrapper page les diffÃ©rentes pages-->
	<section class="lily-main-content">
{%verbatim%}
				<% if ( id !== 'NULL' ) { %>
					<div class="lily-faq-link lily-cst-faq-title-link lily-back-link"><a href="#/faq/<%=parent%>" data-reverse="true"><%=title%><p><i class="lily-icon lily-icon-arrow-left"></i>Retour</p></a></div>
				<% } %>
		<ul>
		<% _.each(faqs, function(faq) { %>
			<li class="lily-faq-link lily-cst-faq-link">
				<a href="javascript:;" data-id="<%=faq.id%>" data-parent="<%=id%>" data-type="<%=faq.type%>"><%=faq.title%>
				<span class="lily-icon lily-icon-arrow-right"></span></a>
			</li>
		<% }) %>
{%endverbatim%}
		</ul>
	</section>
</div>
